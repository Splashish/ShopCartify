         box-shadow: 0px 0px 10px 0px rgba(0,0,0,0.1);
         border-radius: 5px;
       }
       h1 {
         text-align: center;
         color: #333;
       }
       p {
         font-size: 18px;
         color: #666;
       }
       .order-history, .shop-again {
         display: flex;
         justify-content: space-between;
         align-items: center;
         background-color: #f3f3f3;
         padding: 10px;
         border-radius: 5px;
         margin-bottom: 10px;
       }
       .order-history button, .shop-again button {
         background-color: #ffeb3b;
         color: #333;
         border: none;
         padding: 10px 20px;
         text-decoration: none;
         border-radius: 5px;
         font-size: 18px;
       }
       .order-history button:hover, .shop-again button:hover {
         background-color: #ffc107;
       }
       .out-of-stock {
            position: relative;
        }
 
        .out-of-stock::after {
            content: "Out of Stock";
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(255, 0, 0, 0.5);
            display: flex;
            align-items: center;
            justify-content: center;
            color: white;
        }.value{
