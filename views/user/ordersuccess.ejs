     <style>
            @keyframes checkmark {
        0% { stroke-dashoffset: 50px }
        100% { stroke-dashoffset: 0 }
      }

      .checkmark-circle {
        stroke-dasharray: 50px;
        stroke-dashoffset: 50px;
        animation: checkmark 0.5s linear forwards;
      }

      .checkmark-path {
        stroke-dasharray: 100px;
        stroke-dashoffset: 100px;
        animation: checkmark 0.5s linear forwards;
      }

      .success-message {
        text-align: center;
        color: #333;
      }

      .success-message h1 {
        font-size: 3em;
        line-height: 1em;
        text-transform: uppercase;
        color: #2ecc71;
      }
      /* above the sucess animations style */
       body {
