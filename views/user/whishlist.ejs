              <h4 class="card-title mb-4"> Wishlist </h4>

              <% if(wishlistItems && wishlistItems.length > 0) { %>
                <% wishlistItems.forEach((wish) => { %>
                  <div class="row gy-3 mb-4">
                    <div class="col-lg-5">
                      <div class="me-lg-5">

                        <div class="d-flex">
                          <img src="<%= wish?.productId.Images[0].slice(6) %>" class="border rounded me-3" style="width: 96px; height: 96px;" />
                          <div class="">
                            <a href="#" class="nav-link"><%= wish?.productId.name %></a>
                            <p class="text-muted"><%= wish?.productId.category %></p>
                          </div>
                        </div>

                      </div>
                    </div>
                    
                    <p class="card-title mb-3"> price: </p>
                    <div class="col-lg-2 col-sm-6 col-6 d-flex flex-row flex-lg-column flex-xl-row text-nowrap">
                      <div class="">
                        <p class="h6">₹<%= wish?.productId.price %>/-</p> <br />
                      </div>
                    </div>
                    <div class="col-lg col-sm-6 d-flex justify-content-sm-center justify-content-md-start justify-content-lg-center justify-content-xl-end mb-2">

                      <form action="/whishlistRemove/<%= wish?.productId._id %>" method="post">
                        <div class="float-md-end">
                          <button type="submit" class="btn btn-light border text-warning icon-hover-danger">
                            <i class="fas fa-heart-broken me-1"></i> Remove
                          </button>
                        </div>
                      </form>
                      
                      <!-- add to cart -->
                      <div class="float-md-end">
                        <button class="btn btn-light border text-warning icon-hover-success"  type="button"  onclick="addToCart('<%= wish.productId._id %>')"> Add to Cart</button>
                      </div>

                    </div>
                  </div>
                <% }) %>
              <% } else { %>
                <p class="text-dark">Wishlist is empty</p>
                <img src="/web 2/images/empty whishlist.jpg" alt="Empty Wishlist" style="margin-left: 250px;" >
              <% } %>
            </div>
  
