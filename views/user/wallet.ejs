                        <thead>
                            <tr>
                                <th scope="col">Transaction Type</th>
                                <th scope="col">Amount</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% walletData.creditAmounts.forEach(amount => { %>
                                <tr class="text-success">
                                    <td>Credited</td>
                                    <td style="color: rgb(7, 177, 41);">â‚¹<%= amount %></td>
                                </tr>
                            <% }); %>
                            <% walletData.debitAmounts.forEach(amount => { %>
                            <tr class="text-danger">
                                <td>Debited</td>
                                <td style="color: red;">â‚¹<%= amount %></td>
                            </tr>
                            <% }); %>
                        </tbody>
                    </table>
                    <div class="pagination">
                        <% if (currentPage > 1) { %>
                          <a href="?page=<%= currentPage - 1 %>" class="btn btn-primary">Previous</a>
                        <% } %>
                      
                        <% for (let i = 1; i <= Math.ceil(walletData.amounts.length ); i++) { %>
                          <a href="?page=<%= i %>" class="btn btn-primary <%= currentPage == i ? 'active' : '' %>"><%= i %></a>
                        <% } %>
                      
                        <% if (currentPage < Math.ceil(walletData.amounts.length )) { %>
                          <a href="?page=<%= currentPage + 1 %>" class="btn btn-primary">Next</a>
                        <% } %>
                      </div>
                </div>
            </div>
        </div>
