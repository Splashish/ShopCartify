  <!-- ---razorpay working script--- -->
  <script>
    document.querySelector('#checkOutForm').addEventListener('submit', async function (event) {
      event.preventDefault();
      var paymentType = document.querySelector('input[name="paymentType"]:checked').value;
      console.log("payment ", paymentType);
  
      if (paymentType === 'NetBanking') {
        var razorpayKey = 'rzp_test_Jbmx59qoOc1tKj';
        var totalPriceInRupees = document.querySelector('input[name="totalPrice"]').value;
        var totalPriceInPaisa = Math.round(parseFloat(totalPriceInRupees) * 100);
  
        fetch('/payPost', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
          },
          body: JSON.stringify({ totalPrice: totalPriceInPaisa }),
        })
        .then(response => response.json())
        .then(order => {
          const options = {
            key: 'rzp_test_Jbmx59qoOc1tKj',
            amount: order.amount,
            currency: order.currency,
            order_id: order.id,
            name: 'ShopCartify',
            description: 'Payment for your orders',
            theme: {
              color: "#FFD333"
            },
            handler: function (response) {
              document.querySelector('#checkOutForm').submit();
            },
            prefill: {
              name: 'User Name',
              email: 'user@example.com',
            },
          };
  
          var rzp = new Razorpay(options);
          rzp.open();
        })
        .catch(error => {
          console.error('Error creating Razorpay order:', error);
        });
      } else if (paymentType === 'CashOnDelivery') {
        document.querySelector('#checkOutForm').submit();
      } else if (paymentType === 'Wallet') {
        const totalPriceInRupees = document.querySelector('input[name="totalPrice"]').value;
        const totalPriceInPaise = Math.round(parseFloat(totalPriceInRupees));
        const walletBalanceInPaise = Math.round(document.querySelector('.wallet-balance').textContent.replace('₹ ', '') * 100);
  
        if (walletBalanceInPaise > totalPriceInPaise) {
          fetch('/walletPay', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json',
              'Authorization': `Bearer ${localStorage.getItem('token')}`
            },
            body: JSON.stringify({ totalPrice: totalPriceInPaise })
          })
          .then(response => response.json())
          .then(data => {
            if (data.success) {
              console.log("inside data.success");
              document.querySelector('#checkOutForm').submit();
            } else {
              console.log("inside error case");
              Swal.fire({ icon: 'error', title: 'Oops...', text: data.error });
            }
          })
          .catch(error => {
            console.error('Error processing wallet payment:', error);
          });
        } else {
          Swal.fire({ icon: 'error', title: 'Oops...', text: 'Your wallet balance is insufficient.' });
        }
      }
    });
  </script>
   
