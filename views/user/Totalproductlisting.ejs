<body>
  <div class="container">
    <div class="row">
        <% if (products){ %>
            <% for(i=0; i < products.length;i++){ %>
                <div class="col-md-3" style="padding:15px;">
                   <div style="display:inline-block; border:solid 1px #808080; padding:15px; border-radius: 15px; box-shadow: 0px 10px 10px -10px rgba(0,0,0,0.5);">
                <div>
                    <img class="img-fluid" alt="Bootstrap template" src="<%= products[i].Images[0]?.slice(6) %>" />
                    <br />
                    <h2 class="float-xs-right">₹<%= products[i].price %></h2>
                    <h2><a href="#"><%= products[i].name %></a></h2>
                    <br />
                    <p class="text-justify"><%= products[i].description %></p>
                </div>
                <br />
                <div class="ratings text-xs-center">
                    <p>
                        <div id="rating2"></div>
                        stocks:<%= products[i].stock %>
                    </p>
                </div>
                <br />
                <div class="btn-ground text-xs-center" style="padding-bottom: 30px">
                  <a  href="/addToCart?id=<%= products[i].id %>"  class="btn btn-primary"><i class="fa fa-shopping-cart"></i> Add</a>
                  <a   href="/productdetails?id=<%= products[i].id %>" class="btn btn-primary" data-toggle="modal" data-target="#productmodal2"><i class="fa fa-info"></i> Info</a>
               </div>
               
            </div>
        </div>
        <% } %>
        <% } %>
    </div>
</div>


